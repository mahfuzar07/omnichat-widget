{
	"name": "omnichat-widget",
	"private": false,
	"version": "0.1.6",
	"author": "Omni Chat",
	"license": "MIT",
	"description": "A customizable React chat widget",
	"type": "module",
	"main": "dist/chat-widget.umd.js",
	"module": "dist/chat-widget.js",
	"types": "dist/main.d.ts",
	"exports": {
		".": {
			"import": "./dist/chat-widget.js",
			"require": "./dist/chat-widget.umd.js"
		},
		"./styles": "./dist/style.css"
	},
	"files": [
		"dist"
	],
	"scripts": {
